                    <td class="text-left padded font8 padded-left" colspan="6">Cargos del Mes</td>
                </tr>
                <!--<% receipt.concepts.forEach(function(concept){ %>-->
                <!--<tr class="text-left font7">-->
                <!--<td class="text-left padded-left" colspan="2"><%= concept.productName %></td>-->
                <!--<td class="text-right" colspan="1"><%= concept.total %></td>-->
                <!--</tr>-->
                <!--<% }); %>-->
                <tr class="text-left font7">
                    <td class="text-left padded-left black_bg white bold">Cant.</td>
                    <td class="text-left padded-left black_bg white bold">Clave</td>
                    <td class="text-left padded-left black_bg white bold">Cve. Unidad</td>
                    <td class="text-left padded-left black_bg white bold">Cve. Servicio</td>
                    <td class="text-left padded-left black_bg white bold"></td>
                    <td class="text-left padded-left black_bg white bold"></td>
                </tr>
                <tr  class="text-left font7">
                    <td class="text-left padded-left black_bg white bold" colspan="4">Descripci&oacute;n</td>
                    <td class="text-left padded-left black_bg white bold">Precio Unit.</td>
                    <td class="text-left padded-left black_bg white bold">Importe</td>
                </tr>

                <!-- For para colocar todos los conceptos -->
                <% invoice.concepts.forEach(function(concept){ %>
                    <tr class="text-left font7">
                        <td class="text-left padded-left"><%= concept.amount %></td>
                        <td class="text-left padded-left"><%= concept.key %></td>
                        <td class="text-left padded-left"><%= concept.unitKey %></td>
                        <td class="text-left padded-left"><%= concept.serviceKey %></td>
                        <td class="text-left padded-left"></td>
                        <td class="text-left padded-left"></td>
                    </tr>
                    <tr  class="text-left font7">
                        <td class="text-left padded-left" colspan="4"><%= concept.description %></td>
                        <td class="text-right"><%= concept.unitCost %></td>
                        <td class="text-right"><%= concept.total %></td>
                    </tr>
                <% }); %>
                <!-- Fin del for -->

                <tr>
                    <td colspan="6">&nbsp;</td>
                </tr>
                <tr>
                    <td colspan="4"></td>
                    <td class="bold text-right font8">Subtotal</td>
                    <td class="text-right font7" colspan="1"><%= invoice.subtotal %></td>
                </tr>
                <tr>
                    <td colspan="4"></td>
                    <td class="bold text-right font8">Descuentos</td>
                    <td class="text-right font7">-<%= invoice.discount %></td>
                </tr>
                <tr>
                    <td colspan="4"></td>
                    <td class="bold text-right font8">IVA 16%</td>
                    <td class="text-right font7"><%= invoice.taxes %></td>
                </tr>
                <tr>
                    <td colspan="4">&nbsp;</td>
                    <td class="bold text-right font8">Total</td>
                    <td class="text-right font7"><%= invoice.total %></td>
                </tr>
                <tr>
                    <td colspan="3"></td>
                    <td class="bold text-right font8" colspan="2">Cargos del Mes</td>
                    <td class="text-right font7"><%= invoice.total %></td>
                </tr>
                <tr>
                    <td class="text-right font5" colspan="6"><%= invoice.totalInText %></td>
                </tr>
            </table>
            <table border="1" class="table table-bordered" style="border-color: #ff0000;">
                <tr class="rounded-l rounded-r black_bg red bold font8">
                    <td class="text-left padded padded-left" colspan="2">Estado de cuenta</td>
                    <td style="color: white;"></td>
                </tr>
                <% balance.lastMovements.forEach(function(movement){ %>
                <tr class="text-left font7">
                    <td class="text-left padded-left">
                        <%= movement.type %>
                    </td>
                    <td class="font7 text-center">
                        <%= movement.date %>
                    </td>
                    <td class="text-right">
                        <% if(movement.type === 'Recibo') { %>
                        <strong style="color: red"><%= movement.total %></strong>
                        <% } else { %>
                        <%= movement.total %>
                        <% } %>
                    </td>
                </tr>
                <% }); %>
                <tr>
                    <td class="bold text-right font8">
                        Saldo al Corte
                    </td>
                    <td class="text-right">
                        <span class="pull-right font8"><%= currentDate %></span>
                    </td>
                    <td class="text-right font7"><%= balance.total %></td>
                </tr>
                <tr>
                    <td></td>
                    <td class="text-right font5">
                    </td>
                </tr>
            </table>
        </td>
    </tr>
</table>

<table>
    <tr>
        <td class="black_bg rounded-l rounded-r white text-center" id="aumenta">
